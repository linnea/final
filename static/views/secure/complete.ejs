     <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0-rc.1/angular.min.js"></script>
</head>
<body>
    <div class="container">
        <div class="row">
            <h1>Welcome <%= locals.user.displayName || (locals.user.FirstName + ' '+ locals.user.LastName) %> </h1>
            <a href="/logout" class="btn btn-primary" role="button">Log out</a>
        </div>
        <div class="row">
            <h3>First Name</h3>
            <%=locals.user.FirstName%>
            
            <h3>Last Name</h3>
            <%= locals.user.LastName %>
            
            <h3>Email Address</h3>
            <%= locals.user.Email %>
            <h3>
        </div>
            
        </div>
    </div>
